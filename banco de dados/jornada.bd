- Cria um novo banco de dados chamado 'jornada.db'
sqlite3 jornada.db
-- Abre o SQLite e cria a tabela 'usuarios'
CREATE TABLE usuarios (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    usuario TEXT NOT NULL,
    senha TEXT NOT NULL
);
-- Insere alguns dados de exemplo na tabela 'usuarios'
INSERT INTO usuarios (usuario, senha) VALUES ('usuario1', 'senha1');
INSERT INTO usuarios (usuario, senha) VALUES ('usuario2', 'senha2');
-- Seleciona e exibe os dados da tabela 'usuarios'
SELECT * FROM usuarios;

